{
  "name": "ng-indexeddb-signals",
  "version": "1.0.0",
  "description": "Angular 20+ module for IndexedDB with signal-based API",
  "type": "module",
  "packageManager": "pnpm@8.15.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.lib.json",
    "build:demo": "tsc -p tsconfig.demo.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,html,css,scss,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,html,css,scss,json,md}\"",
    "clean": "node scripts/clean.js",
    "clean:all": "node scripts/clean.js --all",
    "prepare": "husky"
  },
  "keywords": [
    "angular",
    "indexeddb",
    "signals",
    "reactive",
    "storage",
    "browser",
    "database"
  ],
  "author": "quinnjr",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quinnjr/ng-indexeddb-signals.git"
  },
  "bugs": {
    "url": "https://github.com/quinnjr/ng-indexeddb-signals/issues"
  },
  "homepage": "https://github.com/quinnjr/ng-indexeddb-signals#readme",
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "^18.0.0",
    "@angular-eslint/template-parser": "^18.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@eslint/js": "^9.0.0",
    "@playwright/test": "^1.48.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^2.1.8",
    "angular-eslint": "^18.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.0",
    "happy-dom": "^15.11.6",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "prettier": "^3.3.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^7.2.1",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@angular/core": "^20.0.0",
    "tslib": "^2.8.1"
  }
}
